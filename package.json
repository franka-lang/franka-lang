{
  "name": "franka-lang",
  "version": "1.0.0",
  "description": "The language of collaboration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "franka": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "tsc",
    "build:frontend": "vite build",
    "dev": "ts-node src/cli/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && node scripts/check-coverage.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"public-react/src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"public-react/src/**/*.{ts,tsx}\"",
    "cli": "ts-node src/cli/index.ts",
    "mcp": "ts-node src/mcp/index.ts",
    "web": "ts-node src/web/index.ts"
  },
  "keywords": [
    "franka",
    "programming-language",
    "mcp",
    "language-specification"
  ],
  "author": "franka-lang",
  "license": "MIT",
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.4",
    "@types/react-dom": "^19.2.3",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.2"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.22.0",
    "@types/express": "^5.0.5",
    "@types/js-yaml": "^4.0.9",
    "commander": "^14.0.2",
    "cp-react-tree-table": "^1.1.2",
    "express": "^5.1.0",
    "js-yaml": "^4.1.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "zod": "^3.25.76"
  }
}
